{
  "name": "captable",
  "license": "UNLICENSED",
  "scripts": {
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy": "graph deploy --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/ brok/captable",
    "create-local": "graph create --node http://localhost:8020/ brok/captable",
    "remove-local": "graph remove --node http://localhost:8020/ brok/captable",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 brok/captable",
    "create:brokTest": "graph create --node http://51.13.101.128:8020/ brok/captable",
    "deploy:brokTest": "yarn prepare:stage && graph deploy --node http://51.13.101.128:8020/ --ipfs http://51.13.101.128:5001 brok/captable",
    "remove:brokTest": "graph remove --node http://51.13.101.128:8020/ brok/captable",
    "reset:brokTest": "yarn remove-brok && yarn create-brok && yarn deploy-brok",
    "prepare:stage": "mustache config/brokStage.json subgraph.template.yaml > subgraph.yaml",
    "prepare:test": "mustache config/brokTest.json subgraph.template.yaml > subgraph.yaml",
    "create:brokStage": "graph create --node http://51.120.95.118:8020/ brok/captable",
    "deploy:brokStage": "yarn prepare:stage && graph deploy --node http://51.120.95.118:8020/ --ipfs http://51.120.95.118:5001 brok/captable",
    "remove:brokStage": "graph remove --node http://51.120.95.118:8020/ brok/captable",
    "reset:brokStage": "yarn remove-brok && yarn create-brok && yarn deploy-brok"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.20.0",
    "@graphprotocol/graph-ts": "0.20.0",
    "mustache": "^4.2.0"
  }
}